# my-programs
def count_substring(string, sub_string):
    c=0
    n=len(string)-2
    n2 = len(sub_string)
    for i in range(0,n):
      if string[i:i+n2]==sub_string:
        c=c+1 
    return c

if __name__ == '__main__':
    string = raw_input().strip()
    sub_string = raw_input().strip()
    
    count = count_substring(string, sub_string)
    print count
