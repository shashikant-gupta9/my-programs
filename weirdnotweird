#!/bin/python

import math
import os
import random
import re
import sys



n = int(input())
    
var = n%2

if n>0 and n<101 :
     if var!=0:
          print("Weird")
     elif var==0 and n>1 and n<6: print("Not Weird") 
     elif var==0 and n > 5 and n<21:print("Weird")  
     elif var==0 and n>20:  print("Not Weird") 
     else:print("oops failed")
     
 
     
 
