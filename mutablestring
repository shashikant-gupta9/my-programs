# my-programs
def mutate_string(string, position, character):
          
         # print string[:position] + character + string[position+1:]

     s = list(string)
     s[position] = character
     string ="".join(s)
     return string  


if __name__ == '__main__':
    s = raw_input()
    i, c = raw_input().split()
    s_new = mutate_string(s, int(i), c)
    print s_new
